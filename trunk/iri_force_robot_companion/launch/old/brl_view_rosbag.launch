<!-- -->

<launch>

  <param name="/use_sim_time" value="True"/>
	<node  pkg="rosbag" 
        type="play" 
        name="rosbag" 
        args=" --pause -r 1 --clock $(find iri_force_robot_companion)/rosbag/data.bag" />

 <!-- tibi_dabo urdf model -->
  <param name="robot_description" command="cat $(find tibi_dabo_base)/model/tibi_dabo_base.urdf" />

  <!-- fme environment -->
<!--  launches 3d mesh map, located at 0,0,0 
<include file="$(find tibi_dabo_base)/launch/fme.launch" />
<include file="$(find tibi_dabo_base)/launch/brl.launch" />
-->

  <node pkg ="rviz" 
        name="rviz" 
        type="rviz" 
        args="-d $(find iri_force_robot_companion)/config/fme_view.vcg"
         />

</launch>


