<launch>

  <param name="/use_sim_time" value="True"/>
	<node  pkg="rosbag" 
        type="play" 
        name="rosbag" 
        args="--clock /home/gferrer/projects/hrimr/robot_companion_JOURNAL2013/media_17_dec/rosbags/data.bag" />

 <!-- tibi_dabo urdf model -->
  <param name="robot_description" command="cat $(find tibi_dabo_base)/model/tibi_dabo_base.urdf" />


    <!-- test nodes here, or include this whole launch -->
    <!-- load 2d map -->
    <node pkg ="map_server" 
          type="map_server" 
          name="map_server" 
          args="$(find iri_force_robot_companion)/map/brl_dec.yaml">
      <param name="frame_id" value="/map" />
    </node>


  <node pkg ="rviz"
        type="rviz"
        name="rviz"
        args="-d $(find iri_force_robot_companion)/config/dabo_companion.vcg">
  </node>

</launch>
